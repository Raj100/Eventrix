{
  "expo": {
    "name": "Eventrix",
    "slug": "eventrix",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventrix.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.eventrix.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to personalize your custom designs.",
          "cameraPermission": "The app uses your camera to capture designs."
        }
      ],
      "expo-router"
    ],
    "scheme": "eventrix",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ff84387-8660-404e-a482-227cc921050a"
      }
    },
    "owner": "raj1000"
  }
}
